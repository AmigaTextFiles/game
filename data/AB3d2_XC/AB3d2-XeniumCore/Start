assign tkg1: ""
assign tkg2: tkg1:
assign ab3: tkg1:
assign sfx: tkg1:
echo "tkg1: tkg2: ab3: and sfx: all assigned to this directory"
echo ""
if exists tkg1:tkgpatch
   echo "Type TKGPatch C2P in the window below when this window closes"
else
   echo "Type TKG in the window below when this window closes"
endif

tkg1:
newshell CON:20/200/600/200/StartWindow/CLOSE

wait 1
