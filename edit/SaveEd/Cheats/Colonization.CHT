Name = Colonization, Amiga (1.1)
Author = Rune J. Keller

;1.1 - Added group names
;1.0 - First release

FileName = colony#?.sav
And
Size = 24701
And
Match = COLONIZE
End

Option = Year
Info = 1280 will be added to this value
Address = $1a
Length = 1
Value = 1

Step = 52
Group = Tribe
Do = 4
	Option = Ruler
	Address = $9e
	Length = 23
	Value = 3
Loop

Step = 52
Group = Tribe
Do = 4
	Option = Land
	Address = $b6
	Length = 23
	Value = 3
Loop
