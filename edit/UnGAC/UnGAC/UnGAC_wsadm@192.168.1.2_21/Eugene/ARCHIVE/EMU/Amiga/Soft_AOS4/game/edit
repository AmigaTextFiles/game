; $VER: UnGAC 0.1

FailAt 20
Version >NIL:

If $kickstart gt 45
   If $workbench not eq "40.0"
      ; Kickstart 53.70, Workbench 53.14
      ungac_aos4.exe -v Example/Cricket-Crazy.sna | more
   Else
      ; Kickstart 51.51, Workbench 40.0
      ungac_aros-i386.exe -v Example/Cricket-Crazy.sna | more
   EndIf
Else
      ; Kickstart less than V50
      ungac_aos3.exe -v Example/Cricket-Crazy.sna | more
EndIf
