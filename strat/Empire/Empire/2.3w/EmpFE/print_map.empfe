/*
 * print_map.empfe - This macro will display a map with the given
 * coordinates.
 */

/* trace ?r*/

parse arg up_row':'down_row','left_col':'right_col

n = 0

type = 1
line = get_map_line(up_row,left_col,down_row,right_col,type,n)
say ""

do while line ~= 'done'
    say line
    n = n + 1
    line = get_map_line(up_row,left_col,down_row,right_col,type,n)
    end

exit(0)



