set mid `getbestmode 640 480 8`
assign >nil: USR: "" add
assign >nil: GG: "" add
if $mid eq -1
   run >nil: mss MoonBuggy a f=topaz/8 from moon-buggy.2
else
   failat 999
   mss MoonBuggy f=topaz/8 id=$mid from moon-buggy.2
   if $rc eq 666
      run >nil: mss MoonBuggy a f=topaz/8 from moon-buggy.2
   endif
endif
