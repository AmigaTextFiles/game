NAME	= SolidGold
CC	= gcc
COPTS	= -O2
AS	= vasmm68k_mot
ASOBJ	= -Fvobj
ASBIN	= -Fbin
ASOPTS	= -quiet -x -DCOPPERBACK -DBOOTLOGO -DKILLOS -DTDFORMAT
ASINC	= -I /usr/local/vbcc/sdk/NDK_3.9/include_i
LD	= vlink
LDOPTL  = -brawbin1 -minalign 2
LDOPTG  = -brawseg -q -minalign 2
RM	= rm -f
CAT	= cat > $@
DO_AS	= $(AS) $(ASOBJ) $(ASOPTS) $(ASINC) -o $@ $<

include make.common
