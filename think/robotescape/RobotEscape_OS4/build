#!/bin/sh
gcc *.c mt/*.c -o robot -Os -s -W -Wall `sdl-config --cflags --libs` -lSDL_mixer -lSDL_ttf -lSDL_image

os4:
(remebr to use SH!)

gcc -newlib -Wall -Isdk:local/newlib/include/SDL  -O2 -o ROBOTOS4.exe *.c mt/*.c -newlib -lSDL_mixer -lvorbisfile -lvorbis -logg -lSDL_image -ljpeg -lpng -lSDL_ttf -lfreetype -lSDL -lz -lauto -lpthread

