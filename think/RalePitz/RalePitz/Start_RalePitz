echo " Erstelle Fontsordner im Ram: !!"
makedir ram:fonts
copy fonts  ram:fonts all quiet
echo " Lenke den Fonts-Ordner auf Ram:Fonts um."
sys:c/assign Fonts:     ram:Fonts
echo " Lade das Programm."
RalePitz
echo " Lenke den Fonts-Ordner auf Sys um."
sys:c/assign Fonts:  Sys:Fonts
echo " Lösche Ram:Fonts !!"
delete ram:fonts all
delete ram:fonts
