.key filename


if exists c:vt
  c:vt <filename>
  quit
  endif

if exists c:ppshow
  c:ppshow <filename>
  quit
  endif

if exists c:mostra
  c:mostra <filename>
  quit
  endif

if exists sys:utilities/Multiview
  sys:utilities/Multiview SCREEN <filename>
  quit
  endif

if exists sys:utilities/display
  sys:utilities/display <filename>
  quit
  endif

if exists sys:utilities/display39
  sys:utilities/display39 <filename>
  quit
  endif

if exists c:view
  c:view <filename>
  quit
  endif

if exists c:iview
  c:iview <filename>
  quit
  endif

if exists c:show
  c:show <filename>
  quit
  endif
