stack 1000000

ShogoLauncher >NIL:
if exists ShogoPreStart
    execute ShogoPreStart
endif

if exists T:ShogoCmdLine
    execute T:ShogoCmdLine
    delete t:ShogoCmdLine
endif

if exists ShogoPostRun
    execute ShogoPostRun
endif

