.KEY d

echo ""
echo ""
echo "--------> installing MEGA-TYPHOON to directory <d>"
makedir <d>
copy df0:mega-typhoon <d>
copy df0:prgfx <d>/prgfx all
echo ""
echo ""
echo "-----------------------------------------------------------------"
echo ""
echo "   to play MEGA-TYPHOON, type the following at the CLI-prompt"
echo ""
echo ""
echo "            > cd <d>"
echo ""                   
echo "            > mega-typhoon"
echo ""
echo "------------------------------------------------------------------"
echo ""
echo ""
