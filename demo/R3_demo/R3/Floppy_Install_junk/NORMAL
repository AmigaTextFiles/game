echo "CD R3"                 >R3_DEMO:s/startup-sequence
echo "failat 21"             >>R3_DEMO:s/startup-sequence
echo "fastmemfirst >nil:"    >>R3_DEMO:s/startup-sequence
echo "assign env: sys: >nil:">>R3_DEMO:s/startup-sequence


; Note:  "rock3" is run WITHOUT the "-lowchip" option

echo "run >nil: rock3"       >>R3_DEMO:s/startup-sequence

echo "endcli >nil:"          >>R3_DEMO:s/startup-sequence
echo "Startup-sequence now altered.  Reboot to play with NORMAL settings."
echo ""
echo "Better sound effects will be used - requiring slightly more CHIP memory."
