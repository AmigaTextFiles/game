echo "CD R3"                     >R3_DEMO:s/startup-sequence
echo "failat 21"                 >>R3_DEMO:s/startup-sequence
echo "fastmemfirst >nil:"        >>R3_DEMO:s/startup-sequence
echo "assign env: sys: >nil:"    >>R3_DEMO:s/startup-sequence


; Note:  The "-lowchip" option after "rock3" changes the mode.

echo "run >nil: rock3 -lowchip"  >>R3_DEMO:s/startup-sequence

echo "endcli >nil:"              >>R3_DEMO:s/startup-sequence
echo "Startup-sequence now altered.  Reboot to play with LOW_CHIP settings."
echo ""
echo "Feeble sound effects will be used - requiring slightly less CHIP memory."
