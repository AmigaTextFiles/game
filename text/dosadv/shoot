.key at/a
if not "$<at>" eq "$cr"
echo "It isn't here!"
skip end
endif
if not "<at>" eq "lock"
echo "Shooting at the <at> won't help."
skip end
endif
if not "$pistol" eq "-1"
echo "You haven't got a gun."
skip end
endif
if "$lo" eq "open"
echo "You have run out of bullets."
skip end
endif
echo "You shoot at the lock and it breaks open."
setenv lo open
lab end
