echo "You are carrying the following:"
setenv carry no
if "$pistol" eq "-1"
echo "A pistol."
setenv carry yes
endif
if "$moonroot" eq "-1"
echo "A sprout of moonroot."
setenv carry yes
endif
if "$powerplant" eq "-1"
echo "A sprout of powerplant."
setenv carry yes
endif
if "$dreamberry" eq "-1"
echo "A sprout of dreamberry."
setenv carry yes
endif
if "$key" eq "-1"
echo "A key."
setenv carry yes
endif
if "$rock" eq "-1"
echo "A $sharp piece of rock."
setenv carry yes
endif
if "$carry" eq "no"
echo "Nothing at all."
endif
