.key item/a,to/a
if not "$<item>" eq "-1"
echo "You haven't got it!"
skip end
endif
if not "<item>" eq "powerplant"
echo "Nobody wants it!"
skip end
endif
if not "$<to>" eq "$cr"
echo "It isn't here!"
skip end
endif
if not "<to>" eq "guard"
echo "It doesn't need it!"
skip end
endif
echo "The guard accepts the powerplant and gives you the key to the cabin."
setenv powerplant 0
setenv key -1
lab end
