.key item/a,with/a
if not "<item>" eq "rock"
echo "It cannot be sharpened!"
skip end
endif
if not "$rock" eq "-1"
echo "You haven't got it!"
skip end
endif
if not "<with>" eq "boulder"
echo "It cannot be sharpened with!"
skip end
endif
if not "$boulder" eq "$cr"
echo "It isn't here!"
skip end
endif
if "$sharp" eq "sharp"
echo "You have already done so!"
skip end
endif
echo "You sharpen the rock with the boulder."
setenv sharp sharp
lab end
