.key item/a
if not "$<item>" eq "$cr"
echo "It isn't here!"
skip end
endif
if not "<item>" eq "bookshelf"
echo "Pushing the <item> won't help."
skip end
endif
if not "$pistol" eq "0"
echo "You have already pushed it as far as it can go."
skip end
endif
echo "You push the bookshelf and cause it to tremble. A pistol falls to the"
echo "floor from it."
setenv pistol $cr
lab end
