.key item/a
if not "$<item>" eq "$cr"
echo "It isn't here!"
skip end
endif
if not "<item>" eq "door"
echo "Opening the <item> won't help."
skip end
endif
if not "$key" eq "-1"
echo "You haven't got a key."
skip end
endif
if "$do" eq "open"
echo "You have already done so."
skip end
endif
echo "You open the door."
setenv do open
where_am_i
lab end
