;
; Installer by software failure/airsoft `97
; 

echo ""
echo "Welcome to Nemac IV Levelcheater installation"
echo ""
echo noline "Continue (y/n) ? "

Set >NIL: ques ?

If $ques EQ y
  skip yo
endif

echo ""
echo "CU"
quit

Lab yo

echo ""
C:Requestfile >ENV:af TITLE "Select Nemac IV drawer" DRAWERSONLY

Assign af: $af

if exists af:Nemac_4
  skip ok
else
  echo "File Nemac_4 not found"
  quit
endif

Lab ok

Copy >NIL: data af:data
Copy >NIL: ncheat $af all
Copy >NIL: levelcheater $af all
Copy >NIL: levelcheater.inf af:LevelCheater.info
Copy >NIL: delete C:
Copy >NIL: execute C:
Copy >NIL: rename C:

echo ""
echo "Finished!"
quit
 
 
