
SaveSide2	bsr	PrintText
		dc.w	$0002,$0000,$0000

		moveq	#0,D0			;first track
		moveq	#80,D1			;count (tracks to write)
		moveq	#1,D2			;diskside
		moveq	#0,D3			;drivenumber
		lea	$1C00.w,A0		;source memory	
		lea	$73800,A1		;mfm_buffer
		lea	MakeCCTrack(pc),A2	;sub part to do
		bsr	SaveDatas
		bne.b	SS2Error
		rts

***********************************************

SS2Error	bsr	PrintText
		dc.w	$0000,$0002,$0001
		bsr	WaitMouse

		addq.l	#4,SP
		pea	Main(pc)
		rts
