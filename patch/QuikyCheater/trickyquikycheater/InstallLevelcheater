;
; Installer by software failure/airsoft `97
; 

echo ""
echo "Welcome to Tricky Quiky Levelcheater installation"
echo ""
echo noline "Continue (y/n) ? "

Set >NIL: ques ?

If $ques EQ y
  skip yo
endif

echo ""
echo "CU"
quit

Lab yo

echo ""
C:Requestfile >ENV:af TITLE "Select destination drawer" DRAWERSONLY

Assign af: $af

Lab ok

Copy >NIL: qcheat $af all
Copy >NIL: qcheat.dat $af all
Copy >NIL: levelcheater $af all
Copy >NIL: levelcheater.inf af:LevelCheater.info

echo ""
echo "Finished!"
quit
 
 
