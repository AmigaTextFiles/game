assign >nil: env: exists
if warn
    assign env: ram:
endif
assign >nil: t: exists
if warn
    assign t: ram:
endif
;Get report file name
which >nil: requestfile
if warn
    echo "Please enter report file name: "
    set >nil: report ?
else
    set report `requestfile title "Please enter report file name" noicons savemode`
endif

;Report system configuration
echo "*n--- SYSTEM CONFIGURATION ---*n" >$ouput
which >nil: showconfig
if warn
    echo >>$report "Please fill in your system configuration here."
else
    showconfig >>$report
endif

;Add SnoopDos log to report
echo "*n--- SNOOPDOS LOG ---*n" >>$report
type LOG:SnoopDos.log >>$report

;Add Enforcer.log to report
echo "*n--- ENFORCER LOG ---*n" >>$report
type LOG:Enforcer.log >>$report

;Remove temporary log drive
remrad LOG: force
