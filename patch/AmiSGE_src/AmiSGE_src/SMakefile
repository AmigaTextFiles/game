##### options

CC =			SC:C/sc
ASM=			DEVPAC:GenAm -L -D -X -V O+,W-
LD  =			SC:C/slink

#COPTS		=	DEFINE=SGE_AMIGA
COPTS		=

#####

T_SGE		=	dist/AmiSGE
O_SGE		=	main.o cloud.o file.o printf.o misc.o

##### targets

everything:		$(T_SGE)


##### rules

.s.o:
		$(ASM) $*.s

.c.o:
		$(CC) $*.c $(COPTS)

#####

$(T_SGE):		$(O_SGE) file.h printf.h
		$(LD) FROM LIB:c.o $(O_SGE) TO $(T_SGE) LIB LIB:sc.lib LIB:debug.lib SC SD ND NOICONS
