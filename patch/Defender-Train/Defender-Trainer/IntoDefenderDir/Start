assign DEF: ""
mount >nil: DEF:def_rad
makedir >nil: def_rad:c def_rad:s def_rad:libs def_rad:fonts
copy >nil: DEF:s/startup-sequence def_rad:s QUIET
copy >nil: DEF:c def_rad:c ALL QUIET
copy >nil: DEF:libs def_rad:libs ALL QUIET
copy >nil: DEF:fonts def_rad:fonts ALL QUIET
copy >nil: DEF:defreboot ram: QUIET
wait 1
RAM:defreboot
