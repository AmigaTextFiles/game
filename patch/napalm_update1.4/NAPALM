if not exists "ENV:"
 Assign ENV: RAM:
endif
if not exists "T:"
 Assign T: RAM:
endif

if exists "Devs:monitors/PAL"
 DEVS:MONITORS/PAL
endif

assign Napalm: ""

; CD Part

assign Napalm_O: NAPALM_CD:Objects
assign Napalm_G: NAPALM_CD:Graphics
assign Napalm_I: NAPALM_CD:Images
assign Napalm_S: NAPALM_CD:Sounds

; HD Part

assign Napalm_M: missions
assign Napalm_E: exec
assign Napalm_X: store
assign Napalm_Y: setup
assign Napalm_U: units


if exists "ENV:RtgMaster_old"
 delete >NIL: ENV:RtgMaster_old ALL
endif

if exists "ENV:RtgMaster"
 rename ENV:RtgMaster ENV:RtgMaster_old
endif

Makedir >NIL: ENV:RtgMaster
Copy >NIL: Napalm_Y:c2p to ENV:RtgMaster
Copy >NIL: Napalm_Y:c2pcurr to ENV:RtgMaster

Stack 16384
CrimsonCrisis

if exists "ENV:RtgMaster_old"
 delete >NIL: ENV:RtgMaster ALL
 rename >NIL: ENV:RtgMaster_old ENV:RtgMaster
endif

assign Napalm: dismount
assign Napalm_O: dismount
assign Napalm_G: dismount
assign Napalm_I: dismount
assign Napalm_S: dismount
assign Napalm_M: dismount
assign Napalm_E: dismount
assign Napalm_X: dismount
assign Napalm_Y: dismount
assign Napalm_U: dismount
