echo "Preparing game..."
assign fonts: fonts/ add
assign libs: libs/ add
assign devs: devs/ add
assign l: l/ add
assign c: c/ add
setmap d
stack 16384
wait 1
M3
echo "Deassigning paths..."
assign fonts: fonts/ remove
assign libs: libs/ remove
assign devs: devs/ remove
assign l: l/ remove
echo "Cleaned up!"
