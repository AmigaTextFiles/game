Newshell "con:/11/640/200/          SUSHI Quakepatch          "
echo "*N"
Requestfile >env:EXEname TITLE="Chose quake executable to patch"
if warn
   echo >env:msg "Operation canceled....!"
   skip end
endif
if not exists $EXEname
   echo >env:msg "File does not exist....!"
   skip end
endif
Requestfile >env:PATCHname TITLE="Chose patch (060.pch for 060.exe ...)"
if warn
   echo >env:msg "Operation canceled....!"
   skip end
endif
Requestfile >env:EXEdest TITLE="Choose output dest & filename"
if warn
   echo >env:msg "Operation canceled....!"
   skip end
endif
Wpatch $EXEname $EXEdest $PATCHname
echo "*N done! - enjoy"
delete >nil: env:EXEname env:EXEdest env:PATCHname
endcli
