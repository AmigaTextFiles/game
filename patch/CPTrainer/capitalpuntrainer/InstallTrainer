;
; Installer by software failure/airsoft `97
; 

echo ""
echo "Welcome to Capital Punishment Trainer installation"
echo ""
echo noline "Continue (y/n) ? "

Set >NIL: ques ?

If $ques EQ y
  skip yo
endif

echo ""
echo "CU"
quit

Lab yo

echo ""
C:Requestfile >ENV:af TITLE "Select Capital Punishment drawer" DRAWERSONLY

Assign af: $af

if exists af:iii
  skip ok
else
  echo "File III not found"
  quit
endif

Lab ok

Copy >NIL: cpcheat $af all
Copy >NIL: Trainer $af all
Copy >NIL: Trainer.inf af:Trainer.info
Copy >NIL: delete C:
Copy >NIL: execute C:
Copy >NIL: rename C:

echo ""
echo "Finished!"
quit
 
 
