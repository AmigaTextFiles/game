
OD=   ltmp:fms/
CFLAGS= -cimq -v
EXE= T:
LIBS= lib:lc.lib lib:amiga.lib

all: $(EXE)fmsdisk.device

$(EXE)fmsdisk.device : $(OD)tag.o $(OD)fms.o
    blink %(right) SC SD LIB $(LIBS) ADDSYM TO %(left)

$(OD)tag.o : tag.asm
    asm -Iinclude:/asminclude/ -o%(left) %(right)

$(OD)fms.o : fms.c
    lc $(CFLAGS) -o%(left) %(right)

