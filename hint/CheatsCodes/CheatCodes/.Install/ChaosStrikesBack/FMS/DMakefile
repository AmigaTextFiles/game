
#   Since not using normal startup, BSS will not be zerod, and cannot
#   make program resident (can't anyway because there is no way to
#   retrieve the data base register in a residentable program)

OD=   dtmp:fms/
CFLAGS=
EXE=
LIBS= lib:lc.lib lib:amiga.lib

all: $(EXE)fmsdisk.device

$(EXE)fmsdisk.device : $(OD)tag.o $(OD)fms.o
    dcc %(right) -o %(left) -l0 -lc -lamiga -v -s

$(OD)tag.o : tag.asm
    asm -Iinclude:/asminclude/ -o%(left) %(right)

$(OD)fms.o : fms.c
    dcc -c $(CFLAGS) -o %(left) %(right) -md -v

