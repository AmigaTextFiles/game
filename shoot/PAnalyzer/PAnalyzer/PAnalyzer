stack 400000

REQUESTSTRING >env:Address TITLE "address:port" NOGADS
REQUESTSTRING >env:pinterval TITLE "ping interval (seconds)" NOGADS

echo "Analyzing ping ...."
setenv panalyzing 1
 qstat -Ts templ/panalyzer -qws $Address >env:ping1
wait sec $pinterval
   setenv ping2 $ping1
   setenv ping3 $ping1
   setenv ping4 $ping1
   setenv ping5 $ping1
   setenv ping6 $ping1
   setenv ping7 $ping1
   setenv ping8 $ping1
   setenv ping9 $ping1
   setenv ping10 $ping1
   setenv ping11 $ping1
   setenv ping12 $ping1
   setenv ping13 $ping1
   setenv ping14 $ping1
   setenv ping15 $ping1
   setenv ping16 $ping1
   setenv ping17 $ping1
   setenv ping18 $ping1
   setenv ping19 $ping1
   setenv ping20 $ping1

run <>nil: execute gendat_pop.rc
execute gendat_loop.rc



