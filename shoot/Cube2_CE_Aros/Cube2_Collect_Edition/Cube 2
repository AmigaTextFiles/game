; Cube 2 start script

If NOT EXISTS data
    RequestChoice >NIL: Title="Cube 2 Launcher" Body="Directory [data] not found. Make sure you downloaded *Nand installed game's data files.*NCheck ReadMe for more information."  gadgets=OK
    Quit
EndIF

If NOT EXISTS packages
    RequestChoice >NIL: Title="Cube 2 Launcher" Body="Directory [packages] not found. Make sure you downloaded *Nand installed game's data files.*NCheck ReadMe for more information."  gadgets=OK
    Quit
EndIF

If NOT EXISTS preconfigured
	Lua:Amilua "Cube 2 Preconfigure"
	Echo "" > preconfigured
EndIf

Stack 1024000

ChangeTaskPri -1

sauer_client -r > RAM:cube2.log
