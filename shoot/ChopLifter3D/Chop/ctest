
;
; Test which version is the fastest on a CPU
;

findcpu 68000
IF WARN
   game.000  -g -d -z -y
ENDIF

findcpu 68010
IF WARN
   game.000  -g -d -z -y
ENDIF

findcpu 68020
IF WARN
   game.000  -g -d -z -y
   game.020  -g -d -z -y
ENDIF

findcpu 68030
IF WARN
   game.000  -g -d -z -y
   game.020  -g -d -z -y
   game.030  -g -d -z -y
   game.040  -g -d -z -y
ENDIF

findcpu 68040
IF WARN
   game.000  -g -d -z -y
   game.020  -g -d -z -y
   game.030  -g -d -z -y
   game.040  -g -d -z -y
ENDIF

