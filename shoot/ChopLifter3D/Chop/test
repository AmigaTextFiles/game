
; Little script to determine the speed of the game
; runs 10 frames of some modes


findcpu 68000
IF WARN
   echo "68000 CPU Found"

   echo "320x240 4p 16 color WireFrame"       >>ram:a
   game.000                             -z -y >>ram:a


   echo "160x120 4p 16 color Polygones"       >>ram:a
   game.000 -p       -q                 -z -y >>ram:a

   echo "320x240 4p 16 color Polygones"       >>ram:a
   game.000 -p                          -z -y >>ram:a


   echo "160x120 4p Grey shaded ditherd"      >>ram:a
   game.000 -g    -d -q                 -z -y >>ram:a

   echo "320x240 4p Grey shaded ditherd"      >>ram:a
   game.000 -g    -d                    -z -y >>ram:a

   echo "640x240 4p Grey shaded ditherd"      >>ram:a
   game.000 -g    -d -h                 -z -y >>ram:a


   echo "320x240 4p Grey textures shaded dith."  >>ram:a
   game.000 -g    -d -u                 -z -y >>ram:a


   echo "160x120 6p HAM6 True color shaded dith."  >>ram:a
   game.000 -t    -d -q                 -z -y >>ram:a

   echo "320x240 6p HAM6 True color shaded dith."  >>ram:a
   game.000 -t    -d                    -z -y >>ram:a

   echo "320x240 6p HAM6 True color textures shaded dith."  >>ram:a
   game.000 -t    -d -u                 -z -y >>ram:a
ENDIF





