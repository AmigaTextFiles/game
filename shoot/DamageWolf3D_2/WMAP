.key FILE/A
.bra {
.ket }

pcd dh1:dl

dh1:Wolf3d/MapEditor {FILE}

if exists T:MapEditor.out

copy T:MapEditor.out "{FILE}"
del T:MapEditor.out

wait 2		;time for HD to update its file blocks - on KS1.3 must be
		;longer - three/four secs.

dh1:Wolf3d/EXE/Wolf3D-BB.exe {FILE}

S:WMAP "{FILE}"

endif

pcd
