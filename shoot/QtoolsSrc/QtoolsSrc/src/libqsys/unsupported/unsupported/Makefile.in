#
# buildings
#
default:
profile:
coverage:
branch:
all: lib profile coverage					# build the complete package

#
# dists
#
distclean: cleanall						# clean even the configured files
	-rm -f Makefile

backup: Makefile
	cp Makefile Makefile.bak
restore: Makefile.bak
	cp Makefile.bak Makefile
	rm Makefile.bak
