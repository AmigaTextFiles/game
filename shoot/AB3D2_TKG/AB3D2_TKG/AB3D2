; Try to autodetect which CPU to use
Set Engine tkg_release_030
C:CPU CHECK 68060 > NIL:
If $RC EQ 0
	Set Engine tkg_release_060
Else
	C:CPU CHECK 68040 > NIL:
	If $RC EQ 0
		Set Engine tkg_release_040
	Endif
Endif


Stack 262144
Assign AB3: ""
Assign TKG1: ""
Assign TKG2: ""
Assign SFX: ""

Bin/$Engine

; Clean Up
Assign REMOVE AB3:
Assign REMOVE TKG1: ""
Assign REMOVE TKG2: ""
Assign REMOVE SFX: ""

