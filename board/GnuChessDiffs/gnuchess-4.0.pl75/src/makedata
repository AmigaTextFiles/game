.key BOOKOPTS/A,BOOK/A
.bra {
.ket }
If Exists gnuchess.book
    Delete gnuchess.book
EndIf
If Exists gnuchess.input
    Delete gnuchess.input
EndIf
If Exists t:gnuchess.data
    Delete gnuchess.data
EndIf
If Exists gnuchess.data
    Delete gnuchess.data
EndIf
Stack 100000
gzip -cd {BOOK} >gnuchess.book
Echo quit > gnuchess.input
gnuchessr -b gnuchess.book -B t:gnuchess.data {BOOKOPTS} -C 1001 -T 4001 < gnuchess.input
Copy t:gnuchess.data gnuchess.data
Delete gnuchess.book gnuchess.input t:gnuchess.data
