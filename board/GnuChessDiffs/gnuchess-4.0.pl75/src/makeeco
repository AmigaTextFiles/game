If Exists t:step0.eco
    Delete t:step0.eco
EndIf
If Exists t:step1.eco
    Delete t:step1.eco
EndIf
If Exists t:step2.eco
    Delete t:step2.eco
EndIf
If Exists t:gnuchess.eco
    Delete t:gnuchess.eco
EndIf
If Exists gnuchess.eco
    Delete gnuchess.eco
EndIf
Stack 100000
Echo 0 0 >t:step0.eco
ecor -C 1001 -T 4001 -b /book/eco.pgn -B t:step1.eco < t:step0.eco
ecosort binsort t:step1.eco t:step2.eco
ecocvt t:step2.eco t:gnuchess.eco
Copy t:gnuchess.eco gnuchess.eco
Delete t:gnuchess.eco t:step0.eco t:step1.eco t:step2.eco
