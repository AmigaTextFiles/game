OBJS = window.o main.o screen.o mouse.o reqtools.o \
	idcmp.o logic.o

ALLGST = include:all.gst

all: $(OBJS)
	slink	from 	lib:c.o + $(OBJS) to FiveInLine \
		lib	lib:sc.lib + lib:reqtools.lib + lib:amiga.lib + lib:scm.lib

window: window.c
	sc window gst=$(ALLGST)

idcmp: idcmp.c
	sc idcmp gst=$(ALLGST)

screen: screen.c
	sc screen gst=$(ALLGST)

mouse: mouse.c
	sc mouse gst=$(ALLGST)

reqtools: reqtools.c
	sc reqtools gst=$(ALLGST)

logic: logic.c
	sc logic gst=$(ALLGST)

main: main.c
	sc main gst=$(ALLGST)
