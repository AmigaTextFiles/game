echo "Installering af Lær-skak på sys: (opstarts/system disk)"
echo "Needs about
Failat 21
IF EXISTS sys:libs/req.library
  echo "req.library findes i forvejen. Omdøbes til reg.library.old ..."
  rename sys:libs/req.library to sys:libs/req.library.old
ENDIF
echo "Kopierer dansk req.library v2.5 til system disk..."
copy req.library to libs:
echo "Kopierer Lær-skak til system disk..."
copy :PIGbase4#? to sys: all
copy sys:PIGbase4/DANSK/locale to sys:PIGbase4
copy sys:PIGbase4/DANSK/locale.info to sys:PIGbase4
echo "installation slut."
wait 2
