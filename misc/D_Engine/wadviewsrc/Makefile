.PHONY : all
.PHONY : clean

# System configuration
SHELL = ade:bin/sh

# System environment
CC = gcc

CODEGEN = -msmall-code

OPTIMIZATION = -O3 -fomit-frame-pointer

OPTIONS = -s -Wall -noixemul

CFLAGS = $(CODEGEN) $(OPTIMIZATION) $(OPTIONS)



LIB = 

OBJ = playsample.o main.o wadview.o \
	show.o showmap.o 

ARCHIVE = 

WadView: $(OBJ)
	$(CC) $(CFLAGS) -o $@ $^ $(ARCHIVE) $(LIB)

all: clean wadview

clean:
	rm -f playsample.o main.o wadview.o show.o \
	showmap.o wadview


playsample.o: playsample.c 

	$(CC) $(CFLAGS) $(INCL) -c playsample.c

main.o: main.c wadview.h 

	$(CC) $(CFLAGS) $(INCL) -c main.c

wadview.o: wadview.c wadview.h 

	$(CC) $(CFLAGS) $(INCL) -c wadview.c

show.o: show.c 

	$(CC) $(CFLAGS) $(INCL) -c show.c

showmap.o: showmap.c 

	$(CC) $(CFLAGS) $(INCL) -c showmap.c

