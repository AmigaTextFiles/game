$(IF:UP)$(IF:PLAYERS)$(IF:FLAG(-P))IF VAL $pvalue GE "$(PING)"
IF NOT $userexgamedir EQ "$(GAME)"
IF VAL $usermaxpl GE "$(MAXPLAYERS)"
IF VAL "$(MAXPLAYERS)" GE $userminpl
echo ""
echo "$(IPADDR):$(PORT) <$(SERVERNAME)>"
echo "  $(PING)/$(RETRIES)"
echo "  $(GAME) $(MAP) ($(MAXPLAYERS) max)"
$(IF:RULE(teamplay))echo "  tplay:$(RULE:teamplay) dmatch:$(RULE:deathmatch) flimit:$(RULE:fraglimit) tlimit:$(RULE:timelimit) $(RULE:url)"$(ENDIF)
echo "  $(PLAYERS)/$(MAXPLAYERS) players"
$(PLAYERTEMPLATE)$(ENDIF)ENDIF
ENDIF
ENDIF
ENDIF$(ENDIF)$(ENDIF)
