echo	"Kopieren von df0: nach dh0: ...."
echo	""

makedir dh0:Erbe2
makedir dh0:Erbe2/Pictures
makedir dh0:Erbe2/Songs

copy 	df0:Pictures 		dh0:Erbe2/Pictures
copy 	df0:Songs 		dh0:Erbe2/Songs
copy 	df0:Erbe2 		dh0:Erbe2
copy 	df0:Erbe2.GAME 		dh0:Erbe2
copy 	df0:Erbe2.info 		dh0:Erbe2
copy 	df0:ErbeDrawer.info 	dh0:Erbe2.info

cd	dh0:Erbe2

echo	""
echo	"Das SCHMUTZIGE ERBE kann nun von Festplatte aus"
echo	"gestartet werden."
