;	Dissasembly from Blitz2
;
;	MUL32


	movem.l d2-d4,-(a7)	; save these
	moveq #0,d2
	tst.l d0
	bpl _br1
	neg.l d0
	moveq #$ff,d2
_br1:
	tst.l d1
	bpl _br2
	neg.l d1
	not.w d2
_br2:
	move.l d0,d3
	move.l d0,d4
	mulu.w d1,d0
	clr.w d0
	swap d0
	swap d3
	mulu.w d1,d3
	add.l d3,d0
	swap d1
	move.l d4,d3
	mulu.w d1,d4
	add.l d4,d0
	swap d3
	mulu.w d1,d3
	swap d3
	add.l d3,d0
	tst.w d2
	beq _br3
	neg.l d0
_br3:
	movem.l (a7)+,d2-d4
	rts
