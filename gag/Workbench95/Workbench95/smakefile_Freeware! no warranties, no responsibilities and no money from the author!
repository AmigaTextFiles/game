#
#
#

wb95: patch.a main.c
	-@delete main.o
	-@delete wb95
	phxass NOEXE QUIET patch.a
	SC main.c
	sc LINK main.o patch.o to wb95

clean:
	-@delete *.o
	-@delete exe
	-@delete *.lnk

in:
	ci -l -f patch.a
	ci -l -f main.c
	ci -l -f smakefile

out:
	co -l -f -q smakefile
	co -l -f -q patch.a
	co -l -f -q main.c

