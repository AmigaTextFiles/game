assign cm1: dh0:cm1
assign cm2: dh0:cm1/cm2
assign cm3: dh0:cm1/cm3
echo "The Child Murderer"
echo "(C) 1994 M. Zerbo"
echo " "
echo "Developed using Aegis Visionary,"
echo "Published by Oxxi, Inc. 1991"
echo "    (213) 427-1227"
stack 20000
run cm1:omp cm1:mod.music2 quiet mbexit
wait 4
run show -c -s16 cm1:title
wait 6
makedir ram:libs
copy libs:diskfont.library ram:libs
copy cm1:libs/MedPlayer.library ram:libs
assign libs: ram:libs path
assign env: ram:env path
copy cm1:show ram:
path ram:
cm1:cm
