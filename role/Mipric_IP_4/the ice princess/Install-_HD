
(IF (= (EXISTS "MIPRIC:" (NOREQ)) 0)
(
	(ABORT "Please run the install program from the basic package before "
		   "trying to install a game to your hard disk.")
))

(MAKEDIR "MIPRIC:The Ice Princess")
(SET @default-dest "MIPRIC:The Ice Princess")
(COPYFILES (SOURCE "Disk 00.Data") (DEST @default-dest))
(COPYFILES (SOURCE "Disk 01.Data") (DEST @default-dest))
