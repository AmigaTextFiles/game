		dc.l    WA_Flags,WFLG_ACTIVATE!WFLG_SMART_REFRESH!WFLG_DRAGBAR!WFLG_DEPTHGADGET!WFLG_CLOSEGADGET
		dc.l    WA_NewLookMenus,-1
		dc.l    WA_AutoAdjust,-1
		dc.l    TAG_DONE
GebietGad
		dc.w    30,63,165,120
		dc.l    0,0
		dc.w    7
		dc.l    PLACETEXT_ABOVE!NG_HIGHLABEL,0,0
SuchenGad
