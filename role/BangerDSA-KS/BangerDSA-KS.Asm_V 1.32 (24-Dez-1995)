		dc.w    1
		dc.l    PLACETEXT_IN,0,0
SpeSucGad
		dc.w    505,124,29,13
		dc.l    0,0
		dc.w    8
		dc.l    PLACETEXT_LEFT!NG_HIGHLABEL,0,0
UniIntGad
		dc.w    0,0,39,13
		dc.l    0,0
		dc.w    0
		dc.l    PLACETEXT_LEFT!NG_HIGHLABEL,0,0
KrautGad
		dc.w    80,0,247,10
		dc.l    0,0
