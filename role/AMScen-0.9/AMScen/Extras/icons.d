proc dumpIcon(register *char owner; register *uint p)void:
    register uint i;
    register ulong ul;

    writeln("define t_icons proc make", owner, "()list int:");
    writeln("    list int li;");
    writeln();
    writeln("    li := CreateIntArray(8);");
    for i from 0 upto 8 - 1 do
	ul := p*;
	p := p + sizeof(uint);
	ul := ul << 16;
	ul := ul | p*;
	p := p + sizeof(uint);
	writeln("    li[", i, "] := 0b", ul : b : -32, ";");
    od;
    writeln("    li");
    writeln("corp;");
corp;

