# DMakefile for Infocom Debugging Tool 
# (works with DICE 2.06.40)

CC	=	dcc

SRCS	=	alphabet.c check.c decode.c extern.c header.c macros.c main.c \
		object.c recog.c stuffing.c tree.c vars.c vocab.c

OBJS 	= 	alphabet.o check.o decode.o extern.o header.o macros.o main.o \
		object.o recog.o stuffing.o tree.o vars.c vocab.o

DebugTool:	$(OBJS)
		dcc -o DebugTool $(OBJS)

$(OBJS):	$(SRCS)
		dcc -c %(right)

$(OBJS):	frobnitz.h

# (c) 1992 by Sascha Wildner
