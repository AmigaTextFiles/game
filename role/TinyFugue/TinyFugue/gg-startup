stack 50000
assign gg: ""

;********
;Modify the lines below to suit your system
;If you've AmiTCP installed, you can change assign inet: to AmiTCP:
assign inet: gg:inet
;
;Modify this to a path where programs will store their files
;(Lynx will keep there downloaded files, bookmarks,....)
;setenv HOME path
setenv HOME /gg/home
;(Remember that 'path' have to be typed in unix style:
;/device/path
;HOME Directory is GG:Home by default 
;********

assign ade: gg:
setenv TERM amiga-f
setenv PS1=$PWD>

date >ram:GG-datestamp

assign LOCAL: gg:

assign BIN: GG:bin
;assign USR: GG:usr
assign HOME: gg:home
assign ETC: GG:etc
;assign INFO:   GG:info
;assign MAN: GG:man
assign TMP: T:
assign SHARE: gg:share
path BIN:  ADD

;assign include: gg:include
;assign libexec: gg:libexec
assign lib: gg:lib


assign C:   BIN:      ADD
;assign DEVS:   GG:Sys/Devs ADD
;assign LIBS:   GG:Sys/Libs ADD
;assign L:   GG:Sys/L ADD
;assign S:   GG:Sys/S ADD

assign GNU:   GG:   

mount IXPIPE: from devs:MountList.IXPIPE

run <nil: >nil: L:fifo-handler
