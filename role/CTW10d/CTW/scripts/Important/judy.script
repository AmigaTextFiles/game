SET @CURRWALK 0|
:LOOP|
ONGOTO 0 @CURRWALK PT2 PT3 PT4 PT1|

:PT1|
WALKTO 101 44 6 @RES|
IF @RES CHDIR|
GOTO LOOP|

:PT2|
WALKTO 214 157 8 @RES|
IF @RES CHDIR|
GOTO LOOP|

:PT3|
WALKTO 114 265 4 @RES|
IF @RES CHDIR|
GOTO LOOP|

:PT4|
WALKTO 217 332 5 @RES|
IF @RES CHDIR|
GOTO LOOP|

:CHDIR|
MATH @CURRWALK + 1|
MATH @CURRWALK % 5|
GOTO LOOP|

:TOUCH|
