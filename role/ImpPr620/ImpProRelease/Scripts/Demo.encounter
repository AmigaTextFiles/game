/* Demo script for the encounter module */

options results
address IMPENCOUNTER.1

STATUS stat

say stat.NUMMONS
say stat.SELECTED
say stat.MONLEN

SELECTMON 0
GETMON mon

lim = stat.MONLEN - 1

do i = 0 to lim
   say mon.i
end

ADDMON
monnum = RESULT

SELECTMON monnum
GETMON mon

mon.0 = "AREXX"

LOCKLIST
PUTMON mon
UNLOCKLIST
