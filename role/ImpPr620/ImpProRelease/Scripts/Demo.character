/* Sample script for the character module */

options results
address IMPCHAR.1

STATUS stat

say stat.NUMCHARS
say stat.SELECTED
say stat.CHARLEN

SELECTCHAR 0
GETCHAR char

lim = stat.CHARLEN - 1

do i = 0 to lim
   say char.i
end

ADDCHAR
charnum = RESULT

SELECTCHAR charnum
GETCHAR char

char.0 = "AREXX"

LOCKLIST
PUTCHAR char
UNLOCKLIST
