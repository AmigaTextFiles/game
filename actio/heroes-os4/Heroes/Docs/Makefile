install-hDATA: $(h_DATA)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(hdir)
	@list='$(h_DATA)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f="`echo $$p | sed -e 's|^.*/||'`"; \
	  echo " $(hDATA_INSTALL) $$d$$p $(DESTDIR)$(hdir)/$$f"; \
	  $(hDATA_INSTALL) $$d$$p $(DESTDIR)$(hdir)/$$f; \
	done

